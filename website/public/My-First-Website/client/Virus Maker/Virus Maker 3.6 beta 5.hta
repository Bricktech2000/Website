		}
		
		option_window.document.write("<br><input type=\"submit\" id=\"btn\" STYLE=\"background-color:black; color:lime; width:375px;\" value=\"OK\"></body>");
		option_window.document.getElementsByTagName("input")[0].focus();
		
		var returning = "";
		
		option_window.document.getElementById("btn").onclick = function(){
			var output_array = [];
			for(i = 0; i < count; i++){
				output_array[i] = option_window.document.getElementsByTagName("input")[i].value;
			}
			setTimeout(function(){option_window.close();}, 10);
			returning = output_array;
		};
		
		while(returning == ""){
			try{option_window.document;}catch(e){
				var output_array = [];
